        ],
        "parameters": [
          {
            "name": "cartList",
            "in": "body",
            "description": "傳遞結帳前購物車最終的商品列表，點選後會回傳綠界 html",
            "schema": {
              "type": "object",
              "properties": {
                "cartList": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "productId": {
                        "type": "number",
                        "example": 1
                      },
                      "size": {
                        "type": "string",
                        "example": "S"
                      },
                      "color": {
                        "type": "string",
                        "example": "磚桔"
                      },
                      "piece": {
                        "type": "string",
                        "example": "2"
                      }
                    }
                  }
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
